package ldb

/**
 * one simple RDF triple with a subject, a predicate and an object
 */

class RDFTriple {

	private String sub
	private String pred
	private String obj


	RDFTriple(sub, pred, obj){
		this.sub = sub
		this.pred = pred
		this.obj = obj
	}

	public String getSub() {
		return sub;
	}

	public String getPred() {
		return pred;
	}


	public String getObj() {
		return obj;
	}

	static constraints = {
	}
}
